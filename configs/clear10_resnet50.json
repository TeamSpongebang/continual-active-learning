{
  "run_name": "test",
  "save_path": "saved",

  "dataset_name": "clear10",
  "dataset_path": "/workspace/DB/CLEAR",
  "num_classes": 11,
  
  "arch": "resnet50",
  "batch_size": 512,
  "num_epochs": 1,
  "num_workers": 16,
  
  "optimizer": "sgd",
  "optimizer_config":{
    "lr": 0.001,
    "momentum": 0.9,
    "weight_decay": 0.01
  },

  "scheduler": "steplr",
  "scheduler_config":{
    "step_size": 1,
    "gamma": 0.1
  },
  
  "swa_start": 100,
  "swa_anneal_epochs": 50,
  "swa_lr_multiplier": 10.0,
  "swa_scheduler_type": "constant",
  "start_swa_at_end": true,
  
  "init_query_size": 500,
  "eval_query_size": 500,
  "query_size": 500,
  
  "num_episodes": 30,
  "num_ensembles": 5,

  "evaluation_protocol": "streaming"
}