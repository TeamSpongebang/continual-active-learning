{
  "run_name": "al-vit-kraken-0.8",
  "save_path": "saved",

  "dataset_name": "clear10",
  "dataset_path": "/workspace/DB/CLEAR",
  "num_classes": 11,
  
  "arch": "resnet50",
  "batch_size": 256,
  "num_epochs": 50,
  "num_workers": 16,
  
  "trainer": "ensemble",
  "finetune": true,
  "num_ensembles": 5,
  "ensemble_config":{
    "freezing": true,
    "freeze_rate": 0.5,
    "freeze_num": null
  },

  "optimizer": "sgd",
  "optimizer_config":{
    "lr": 0.01,
    "momentum": 0.9,
    "weight_decay": 0.01
  },

  "scheduler": "steplr",
  "scheduler_config":{
    "step_size": 1,
    "gamma": 0.9
  },
  
  "active_learning": true,
  "query_type": "ensentropy",
  "query_size": 0.8,

  "evaluation_protocol": "streaming"
}